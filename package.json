{
  "name": "tennis",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "author": "Eladio Caritos",
  "description": "Connect with tennis players in your local clubs",
  "appMeta": {
    "supportEmail": "support@caritos.com",
    "website": "caritos.com/tennis"
  },
  "scripts": {
    "start": "expo start",
    "start:prod": "mv .env.development .env.development.bak 2>/dev/null || true && NODE_ENV=production expo start; mv .env.development.bak .env.development 2>/dev/null || true",
    "start:dev": "NODE_ENV=development expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "type-check": "npx tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:unit:watch": "jest --selectProjects unit --watch",
    "test:integration:watch": "jest --selectProjects integration --watch",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:challenge": "node scripts/test-challenge-simple.js",
    "test:rls": "node scripts/test-rls-policy.js",
    "test:realtime": "node scripts/test-realtime-subscriptions.js",
    "test:realtime:prod": "node scripts/test-realtime-subscriptions.js --prod",
    "test:realtime:simple": "node scripts/test-realtime-simple.js",
    "test:realtime:simple:prod": "node scripts/test-realtime-simple.js --prod",
    "test:matches": "node scripts/test-matches-realtime.js",
    "test:matches:prod": "node scripts/test-matches-realtime.js --prod",
    "diagnose": "node scripts/diagnose-realtime.js",
    "diagnose:prod": "node scripts/diagnose-realtime.js --prod",
    "check:deps": "node scripts/detect-dependency-issues.js",
    "check:deps:fix": "node scripts/detect-dependency-issues.js --fix",
    "db:generate-types": "node scripts/db-generate-types.js",
    "db:diff": "node scripts/db-diff.js",
    "db:push": "node scripts/db-push.js",
    "db:reset": "supabase db reset",
    "env:dev": "node scripts/switch-env.js dev",
    "env:prod": "node scripts/switch-env.js prod",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:contact-sharing": "jest tests/e2e/integration/contact-sharing-complete.test.tsx --coverage",
    "test:contact-sharing:watch": "jest tests/e2e/integration/contact-sharing-complete.test.tsx --watch",
    "test:100-coverage": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":100,\"functions\":100,\"lines\":100,\"statements\":100}}'",
    "test:monitor": "node scripts/intelligent-test-monitor.js start",
    "test:monitor:contact-sharing": "node scripts/intelligent-test-monitor.js contact-sharing",
    "test:monitor:report": "node scripts/intelligent-test-monitor.js report",
    "test:automated": "node scripts/automated-testing.js start",
    "test:automated:contact-sharing": "node scripts/automated-testing.js contact-sharing",
    "e2e": "./scripts/run-e2e-tests.sh",
    "e2e:contact-sharing": "maestro test tests/e2e/flows/14-contact-sharing-system.yaml",
    "test:orchestrate": "node scripts/advanced-test-orchestrator.js",
    "db:validate": "node scripts/validate-functions.js",
    "db:validate-schema": "node scripts/validate-database-schema.js",
    "db:check": "npm run db:validate && echo 'âœ… Database functions validated'",
    "precommit": "npm run lint && npm run type-check && npm run db:validate",
    "test:orchestrate:smoke": "node scripts/advanced-test-orchestrator.js smoke",
    "test:orchestrate:regression": "node scripts/advanced-test-orchestrator.js regression",
    "test:orchestrate:full": "node scripts/advanced-test-orchestrator.js full",
    "test:orchestrate:ci": "node scripts/advanced-test-orchestrator.js ci",
    "test:orchestrate:release": "node scripts/advanced-test-orchestrator.js release",
    "coverage:analyze": "node scripts/coverage-monitor.js analyze",
    "coverage:monitor": "node scripts/coverage-monitor.js monitor",
    "coverage:gateway": "node scripts/coverage-monitor.js gateway",
    "coverage:baseline": "node scripts/coverage-monitor.js baseline",
    "build:increment": "node scripts/increment-build.js",
    "build:ios": "npm run build:increment && npx eas build --platform ios --profile production",
    "build:ios-preview": "npm run build:increment && npx eas build --platform ios --profile preview",
    "e2e:record": "export PATH=\"$PATH\":\"$HOME/.maestro/bin\" && maestro studio",
    "e2e:check": "./scripts/check-maestro.sh",
    "e2e:devices": "./scripts/run-device-tests.sh",
    "e2e:devices:iphone": "./scripts/run-device-tests.sh iphone",
    "e2e:devices:ipad": "./scripts/run-device-tests.sh ipad",
    "app-store:metadata": "./scripts/update-app-metadata.sh",
    "app-store:safety": "echo 'Safety features implemented: Report/Block system, Community Guidelines'",
    "wiki:update": "./scripts/update-wiki.sh",
    "wiki:generate-faq": "node scripts/generate-wiki-faq.js",
    "wiki:generate-legal": "node scripts/generate-legal-docs.js",
    "checks:pre-commit": "npm run check:deps && npm run lint && npm run type-check && npm run test:unit",
    "checks:pre-push": "npm run check:deps && npm run lint && npm run type-check && npm run test:all && npm run e2e:check",
    "checks:all": "npm run check:deps && npm run lint && npm run type-check && npm run test:all && npm run e2e",
    "bundle:analyze": "npx expo export --dump-assetmap --output-dir ./bundle-analysis",
    "bundle:profile": "EXPO_DEBUG=1 npx expo start",
    "memory:profile": "npx expo start --dev-client",
    "checks:quick": "npm run lint && npm run type-check",
    "checks:fix": "npm run lint:fix && npm run type-check",
    "verify:hooks": "./scripts/verify-hooks.sh",
    "fix:auth": "node scripts/fix-auth-error.js",
    "debug:auth": "node scripts/fix-auth-error.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.5.2",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.52.0",
    "expo": "53.0.22",
    "expo-apple-authentication": "~7.2.4",
    "expo-blur": "~14.1.5",
    "expo-checkbox": "~4.1.4",
    "expo-constants": "~17.1.7",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "^16.1.4",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-radio-buttons-group": "^3.1.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "dotenv": "^17.2.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "husky": "^9.1.7",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3"
  },
  "private": true
}
